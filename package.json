{
  "name": "chess-site-monorepo",
  "private": true,
  "version": "0.1.0",
  "workspaces": ["frontend", "server"],
  "scripts": {
    "install:all": "npm install --workspaces",
    "dev": "npm run dev --workspace frontend",
    "dev:server": "npm run dev --workspace server",
    "dev:full": "concurrently \"npm run dev --workspace server\" \"npm run dev --workspace frontend\"",
    "lint": "npm run lint --workspaces"
  },
  "devDependencies": {
    "concurrently": "^9.1.0"
  }
}
